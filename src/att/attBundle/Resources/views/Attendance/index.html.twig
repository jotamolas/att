{% block attjavascripts %}
    <script>

        $(document).ready(function(){
            
         
          $('#importMasive').click(function(){
              $('#content').load('{{ path('att_plan_importfile') }}');
          });
          
          $('#create').click(function(){
              $('#content').load('{{ path('att_plan_new') }}');
          });
          
          $('#listPlan').click(function(){
              $('#content').load('{{ path('att_plan_list') }}');
          });
          
        });

   </script>
{% endblock %}

