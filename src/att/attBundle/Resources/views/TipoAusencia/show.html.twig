{% extends 'attBundle::base.html.twig' %}

{% block body %}
    <h1>Tipo de Ausencia</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ attipoausencium.id }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ attipoausencium.destipoausencia }}</td>
            </tr>

        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('att_tipoaus_index') }}">Volver</a>
        </li>
        <li>
            <a href="{{ path('att_tipoaus_edit', { 'id': attipoausencium.id }) }}">Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
