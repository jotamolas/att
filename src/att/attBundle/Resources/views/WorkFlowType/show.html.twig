{% extends 'attBundle::base.html.twig' %}

{% block body %}
    <h1>Work Flow Tipologia</h1>

    <table class="table table-striped">
        <tbody>
            <tr>
                <th>Nombre</th>
                <td>{{ atworkflowtype.name }}</td>
            </tr>
            <tr>
                <th>Entityworkflow</th>
                <td>{{ atworkflowtype.entityworkflow }}</td>
            </tr>
            <tr>
                <th>Service Id</th>
                <td>{{ atworkflowtype.serviceid }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('att_workflows_index') }}">Volver</a>
        </li>
        <li>
            <a href="{{ path('att_workflows_edit', { 'id': atworkflowtype.serviceid }) }}">Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Eliminar">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
