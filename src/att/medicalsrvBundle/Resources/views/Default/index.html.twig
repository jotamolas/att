{% extends "appBundle:app:index.html.twig"%}
{% block sidebar %}
    <li>
        <a href="#" class="active"><i class="fa fa-ambulance  fa-fw"></i> 
            <span class="masked">
                {% trans %} Medical Module{% endtrans %}
            </span>    
        </a>
    </li>
    <li class="">
        <a href="#">
            <i class="fa fa-user-md fa-fw"></i>
            <span class="masked">
                <span>
                    {% trans %}Medical Service {% endtrans %}
                </span>
                <span class="fa arrow"></span>
            </span>
        </a>
        <ul class="nav nav-second-level collapse" aria-expanded="false" style="height: 0px;">
            <li>
                <a id="list_medical_service" href="{{ path("medical_service_list",{"mode":app.security.token.providerKey}) }}"> 
