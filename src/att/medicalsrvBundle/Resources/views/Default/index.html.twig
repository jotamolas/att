    </li>
{% endblock %}
{% block content %}
    <div class="row">
        <div class="page-header">
            <h1>{% trans %}Medical Service{% endtrans %} <small>{% trans %} Module{% endtrans %}</small></h1>
        </div>
    </div>
{% endblock %}